# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true

[*.{es,js,json,jsm,jsx,ts,yml}]
indent_style = space
indent_size = 2

[Jenkinsfile]
indent_style = space
indent_size = 4

[*.dockerfile]
indent_style = space
indent_size = 2
