{
  "manifest_version": 2,
  "name": "messaging",
  "version": "1.0",
  "description": "Test messaging between app and web extension",
  "content_scripts": [
    {
      "matches": [
            "*://*.example.com/*"
       ],
      "js": ["messaging.js"]
    }
  ],
  "permissions": [
    "geckoViewAddons",
    "nativeMessaging"
  ]
}
